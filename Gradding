#include<stdio.h>
int main()
{
	int rno;
	char name[20];
	float marks;
	printf("Welcome to Bit Elementary School!\nEnter student's details bellow:-\nName: ");
	scanf("%s",&name);
	printf("Roll number: ");
	scanf("%d",&rno);
	printf("Marks(in %%): ");
	scanf("%f",&marks);
	if (marks>=85.0 && marks<=100.0)
	{
		printf("Grade: A");
	}
	else if (marks>=70.0 && marks<=84.99)
	{
		printf("Grade: B");
	}
	else if (marks>=55.0 && marks<=69.99)
	{
		printf("Grade: C");
	}
	else if (marks>=40.0 && marks<=54.99)
	{
		printf("Grade: D");
	}
	else if (marks<=39.99)
	{
		printf("Grade: F");
	}
	return 0;
}
